; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Chaotic Hammer"
run/main_scene="res://mainMenu.tscn"
config/icon="res://assetsNeeded/gameIcon.png"

[autoload]

Singleton="*res://Singleton.gd"

[display]

window/size/width=450
window/size/height=800
window/size/fullscreen=true
window/handheld/orientation="portrait"
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/reflections/texture_array_reflections.mobile=true
quality/reflections/high_quality_ggx.mobile=true
quality/shading/force_vertex_shading=true
quality/subsurface_scattering/quality=2
quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
